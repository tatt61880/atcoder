func main()
 var n: int :: cui@inputInt()
 var s: [][]char :: #[n][]char
 for i(0, n - 1)
  do s[i] :: cui@input()
 end for

 var d: dict<[]char, bool> :: #dict<[]char, bool>
 for i(0, n - 1)
  for j(0, n - 1)
    if(i <> j)
     do d.add(s[i] ~ s[j], true)
    end if
  end for
 end for
 do cui@print("\{^d}\n")
end func