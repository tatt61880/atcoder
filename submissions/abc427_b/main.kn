func main()
	var n: int :: cui@inputInt()
	
	var a: []int :: #[n + 1]int
	do a[0] :: 1
	for i(1, n)
		do a[i] :: 0
		for j(0, i - 1)
			do a[i] :+ f(a[j])
		end for
	end for
	
	var ans: int :: a[n]
	do cui@print("\{ans}\n")
	
	func f(n: int): int
		var res: int :: 0
		while(n <> 0)
			do res :+ n % 10
			do n :/ 10
		end while
		ret res
	end func
end func
