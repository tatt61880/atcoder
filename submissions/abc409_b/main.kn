func main()
	var n: int :: cui@inputInt()
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
	end for
	
	var ans: int :: 0
	
	for x(1, n)
		var num: int :: 0
		for i(0, n - 1)
			if(a[i] >= x)
				do num :+ 1
			end if
		end for
		if(num >= x)
			do ans :: x
		end if
	end for
	
	do cui@print("\{ans}\n")
end func
