func main()
	var n: int :: cui@inputInt()
	var k: int :: cui@inputInt()
	
	var dic: dict<int, bool> :: #dict<int, bool>
	
	for(1, k)
		var d: int :: cui@inputInt()
		for(1, d)
			var a: int :: cui@inputInt()
			do dic.add(a, true)
		end for
	end for
	
	var ans: int :: n - ^dic
	do cui@print("\{ans}\n")
end func
