func main()
	var h: int :: cui@inputInt()
	var w: int :: cui@inputInt()
	var n: int :: cui@inputInt()
	
	var a: [][]int :: #[h, w]int
	for i(0, h - 1)
		for j(0, w - 1)
			do a[i][j] :: cui@inputInt()
		end for
	end for
	
	var nums: []int :: #[h]int
	for loop(1, n)
		var b: int :: cui@inputInt()
		for i(0, h - 1)
			for j(0, w - 1)
				if(a[i][j] = b)
					do nums[i] :+ 1
					skip loop
				end if
			end for
		end for
	end for
	
	var ans: int :: nums.max()
	do cui@print("\{ans}\n")
end func
