func main()
	var n: int :: cui@inputInt()
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
	end for
	
	do a.sort()
	
	var ans: []int :: #[0]int
	var prev: int :: -1
	for i(0, n - 1)
		if(a[i] <> prev)
			do ans :~ [a[i]]
			do prev :: a[i]
		end if
	end for
	
	do cui@print("\{^ans}\n")
	do cui@print("\{ans.join(" ")}\n")
end func
