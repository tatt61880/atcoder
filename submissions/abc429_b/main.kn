func main()
	var n: int :: cui@inputInt()
	var m: int :: cui@inputInt()
	
	var a: []int :: #[n]int
	var sum: int :: 0
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
		do sum :+ a[i]
	end for
	
	var ans: bool :: false
	
	for i(0, n - 1)
		if(a[i] = sum - m)
			do ans :: true
			break i
		end if
	end for
	
	do cui@print((ans ?("Yes", "No")) ~ "\n")
end func
