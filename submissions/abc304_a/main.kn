func main()
	var n: int :: cui@inputInt()
	var s: [][]char :: #[n][]char
	var a: []int :: #[n]int
	
	var minA: int :: lib@intMax
	var minI: int :: 0
	for i(0, n - 1)
		do s[i] :: cui@inputStr()
		do a[i] :: cui@inputInt()
		if(a[i] < minA)
			do minA :: a[i]
			do minI :: i
		end if
	end for
	
	for i(0, n - 1)
		do cui@print("\{s[(i + minI) % n]}\n")
	end for
end func
