func main()
	var n: int :: cui@inputInt()
	var k: int :: cui@inputInt()
	
	var ans: int :: 0
	for i(1, n)
		do ans :+ f(i) = k ?(1, 0)
	end for
	
	do cui@print("\{ans}\n")
	
	func f(n: int): int
		var res: int :: 0
		while(n <> 0)
			do res :+ n % 10
			do n :/ 10
		end while
		ret res
	end func
end func