func main()
	var s: []char :: cui@inputStr()
	var n: int :: ^s
	
	var ans: float :: 0.0
	
	for i(0, n - 1)
		if(s[i] = 't')
			for j(i + 2, n - 1)
				if(s[j] = 't')
					var num: int :: 0
					for k(i + 1, j - 1)
						if(s[k] = 't')
							do num :+ 1
						end if
					end for
					do ans :: lib@maxFloat(ans, num $ float / (j - i - 1) $ float)
				end if
			end for
		end if
	end for
	
	do cui@print("\{ans}\n")
end func
