func main()
	var n: int :: cui@inputInt()
	var k: int :: cui@inputInt()
	
	var ans: int :: 1
	for i(0, n - 1)
		var a: int :: cui@inputInt()
		if(a > lib@intMax / ans)
			do ans :: 1
		else
			do ans :* a
			if(ans >= 10 ^ k)
				do ans :: 1
			end if
		end if
	end for
	
	do cui@print("\{ans}\n")
end func
