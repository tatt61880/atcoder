func main()
	const n: int :: 5
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
	end for
	
	var cnt: int :: 0
	for i(0, 3)
		if(a[i] > a[i + 1])
			do a[i + 1] :: a[i]
			do cnt :+ 1
		end if
	end for
	var ans: bool :: cnt = 1
	do cui@print((ans ?("Yes", "No")) ~ "\n")
end func
