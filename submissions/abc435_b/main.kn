func main()
	var n: int :: cui@inputInt()
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
	end for
	
	var ans: int :: 0
	for l(0, n - 1)
		for r(l, n - 1)
			var sum: int :: 0
			for i(l, r)
				do sum :+ a[i]
			end for
			for i(l, r)
				if(sum % a[i] = 0)
					skip r
				end if
			end for
			do ans :+ 1
		end for
	end for
	do cui@print("\{ans}\n")
end func
