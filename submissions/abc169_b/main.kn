func main()
	var n: int :: cui@inputInt()
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
	end for
	
	var f: float :: 1.0
	for i(0, n - 1)
		do f :* a[i] $ float
	end for
	if(f > 1.5 * 10.0 ^ 18.0)
		do cui@print("-1\n")
		ret
	end if
	
	var ans: int :: 1
	for i(0, n - 1)
		do ans :* a[i]
	end for
	
	if(ans > 10 ^ 18)
		do ans :: -1
	end if
	
	do cui@print("\{ans}\n")
end func
