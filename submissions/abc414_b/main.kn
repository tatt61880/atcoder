func main()
	var n: int :: cui@inputInt()
	var c: []char :: #[n]char
	var l: []int :: #[n]int
	
	var len: int :: 0
	
	for i(0, n - 1)
		do c[i] :: cui@inputChar()
		do l[i] :: cui@inputInt()
		if(len <= 100)
			do len :+ l[i]
		end if
	end for
	
	if(len <= 100)
		var ans: []char :: #[0]char
		for i(0, n - 1)
			do ans :~ [c[i]].repeat(l[i])
		end for
		do cui@print("\{ans}\n")
	else
		do cui@print("Too Long\n")
	end if
end func
