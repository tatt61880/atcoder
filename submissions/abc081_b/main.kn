func main()
	var n: int :: cui@inputInt()
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
	end for
	
	var ans: int :: 0
	while loop(true)
		for i(0, n - 1)
			if(a[i] % 2 <> 0)
				break loop
			end if
			do a[i] :/ 2
		end for
		do ans :+ 1
	end while
	do cui@print("\{ans}\n")
end func