func main()
	var n: int :: cui@inputInt()
	var a: int :: cui@inputInt()
	var b: int :: cui@inputInt()
	
	var ans: int :: 0
	for i(1, n)
		var x: int :: f(i)
		if(a <= x & x <= b)
			do ans :+ i
		end if
	end for
	
	do cui@print("\{ans}\n")
	
	func f(n: int): int
		var res: int :: 0
		while(n <> 0)
			do res :+ n % 10
			do n :/ 10
		end while
		ret res
	end func
end func