func main()
	var n: int :: cui@inputInt()
	var m: int :: cui@inputInt()
	
	var s: []char :: cui@inputStr()
	var t: []char :: cui@inputStr()
	
	var ans: int :: lib@intMax
	for i(0, n - m)
		var num: int :: 0
		for j(0, m - 1)
			var a: int :: s[i + j] $ int - '0' $ int
			var b: int :: t[j] $ int - '0' $ int
			if(b <= a)
				do num :+ a - b
			else
				do num :+ 10 - b + a
			end if
		end for
		do ans :: lib@min(ans, num)
	end for
	do cui@print("\{ans}\n")
end func
